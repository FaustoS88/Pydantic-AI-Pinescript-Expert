# OpenAI API key (required for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Base URL (optional, defaults to OpenAI's official API)
OPENAI_BASE_URL=https://api.openai.com/v1

# OpenAI Default Model (optional, defaults to gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# OpenAI Embedding Model (optional, defaults to text-embedding-nomic-embed-text-v1.5@f16)
OPENAI_EMBEDDING_MODEL=text-embedding-nomic-embed-text-v1.5@f16

# OpenRouter API key (for LLM queries)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter Default Model (optional, defaults to openai/gpt-4.1-mini)
OPENROUTER_MODEL=openai/gpt-4.1-mini

# Database connection string
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pydantic